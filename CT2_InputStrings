#include <iostream>
#include <string>

int main() {
    std::string first;
    std::string last;

for (int i = 0; i <=2; i++){

    // get user first name
    std::cout << "Enter first name: ";
    std::cin >> first;

    // get user last name
    std::cout << "Enter last name: ";
    std::cin >> last;

    // Display the concatenate response 
    std::cout << "Full Name "<< i + 1 << ": " << first << " " << last << "\n";
    std::cout << "\n";

}

    return 0;
}
